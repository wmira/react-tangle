{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "moduleResolution": "node",
    "lib": ["es5", "es6", "dom"],
    "baseUrl": ".",
    "paths": {
        "react-sidenav": ["src"]
    },
    "declaration": true,
    "types": [
        "jest",
        "node"
    ],
  }
}